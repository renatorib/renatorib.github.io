{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": false,
    "react/display-name": false
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
